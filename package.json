{
  "name": "my-qwik-basic-starter",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adaptors/express/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "node server/entry.express",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint src",
    "postinstall": "prisma generate",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.express",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@auth/core": "^0.4.0",
    "@builder.io/qwik": "0.100.0",
    "@builder.io/qwik-auth": "0.0.5",
    "@builder.io/qwik-city": "0.100.0",
    "@types/compression": "^1.7.2",
    "@types/eslint": "8.37.0",
    "@types/express": "4.17.17",
    "@types/node": "latest",
    "@types/node-fetch": "latest",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "autoprefixer": "^10.4.14",
    "compression": "^1.7.4",
    "eslint": "8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-qwik": "0.100.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "express": "4.18.2",
    "node-fetch": "3.3.1",
    "postcss": "^8.4.21",
    "prettier": "2.8.7",
    "tailwindcss": "^3.3.1",
    "typescript": "5.0.3",
    "undici": "5.21.0",
    "vite": "4.2.1",
    "vite-tsconfig-paths": "4.0.7"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.12.0",
    "class-variance-authority": "^0.5.1",
    "daisyui": "^2.51.5",
    "prisma": "^4.12.0",
    "zod": "^3.21.4"
  }
}
